import java.util.*;
public class Main
{
    int a=5;
    void printF(String msg){
        System.out.println("Inside PrintF function : "+msg);
    }
    int mul(int b, int c)
    {
        return b*c;
    }
    float div(float b, float c)
    {
        return b/c;
    }
	public static void main(String[] args) {
        Scanner scan =new Scanner(System.in);
        Main abc = new Main();
        System.out.println(abc.a);
        //System.out.println("Inside Main function : Aditya and Rakesh");
        abc.printF("Aditya and Rakesh");
        int b=scan.nextInt();
        int c=scan.nextInt();
        System.out.println("Inside Main function " + (b*c));
        abc.printF( (b*c) + "");
        System.out.println("Inside Main function " + abc.mul(b,c));
	    abc.printF( (b/c) + "");
        System.out.println("Inside Main function " + abc.div(b,c));
	}
}
