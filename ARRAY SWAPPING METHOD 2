
import java.util.*;  

public class Main   
{  
  public static void main(String args[])  
  {  
    int i,n;  
    int a[] = {1,2,3};
    int c[]=new int[a.length] ;
    n = a.length;
    for(i=0; i<n; i++)
    {
     c[n-i-1]=a[i];
    }
        System.out.print(" "+c[0]);  
        System.out.print(" "+c[1]);  
        System.out.print(" "+c[2]);  
    }
}


